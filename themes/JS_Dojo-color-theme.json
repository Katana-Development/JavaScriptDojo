    "tokenColors": [
        // Token-Error, Invalid
        {
            "scope": ["token.error-token", "invalid"],
            "settings": {
                "foreground": "#FFffff"
            }
        },
        // Token-Warning
        {
            "scope": "token.warn-token",
            "settings": {
                "foreground": "#FFFF00"
            }
        },
        // Token-Debug/bug
        {
            "scope": "token.debug-token",
            "settings": {
                "foreground": "#1AD240"
            }
        },
        // TOKEN-INFO
        {
            "scope": "token.info-token",
            "settings": {
                "foreground": "#80FFFF"
            }
        },
        /* 

    Anything JSDoc Goes Here ('Temporary -- will be last elements colored') */
        {
            "scope": ["storage.type.class.jsdoc"],
            "settings": {
                "foreground": "#3aa395",
                "fontStyle": ""
            }
        },
        {
            "scope": ["variable.other.jsdoc", "punctuation.definition.block.tag.jsdoc"],
            "settings": {
                "foreground": "#C0B050",
                "fontStyle": "bold"
            }
        },
        /*
    
    COMMENT.* (Comment-Block's, Single-Line Comments, & Comment Puncuation)        */
        {
            "scope": [
                "comment",
                "comment.line.double-slash",
                "comment.block.documentation",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#3F6090",
                "fontStyle": "italic"
            }
        },
        /*
    
    STRING.* (Double Quoted & Single Quoted) */
        {
            "scope": "string.quoted",
            "settings": {
                "foreground": "#4198c0",
                "fontStyle": "italic"
            }
        },
        /*   
    

    STRING.TEMPLATE   */
        {
            "scope": "string.template",
            "settings": {
                "foreground": "#43998e",
                "fontStyle": "italic"
            }
        },
        /*
    
    ENTITY.NAME     */
        {
            "scope": [
                "entity.name.type",
                "entity.name.class",
                "entity.name.method",
                "entity.name.function"
            ],
            "settings": {
                "foreground": "#309BED",
                "fontStyle": "bold"
            }
        },
        /*      

    SUPPORT.*       */
        {
            "scope": [
                "support.type.object.module",
                "support.constant",
                "support.function",
                "support.type",
                "support.type.property-name",
                "support.class",
                "support.class.error.js", // JavaScript Specific
                "support.macro"
            ],
            "settings": {
                "foreground": "#d2e757",
                "fontStyle": "italic"
            }
        },
        // JSON
        {
            "scope": [
                "support.type.property-name.json",
                "support.constant.json",
                "support.function.json",
                "punctuation.definition.template-expression"
            ],
            "settings": {
                "foreground": "#ebb542",
                "fontStyle": ""
            }
        },
        /*
    
    VARIABLE.* [GROUP# 1]   */
        {
            "scope": [
                "variable",
                "variable.other",
                "variable.name",
                "variable.member",
                "variable.other.constant",
                "variable.other.readwrite",
                "variable.language",
                "variable.other.property",
                "variable.parameter",
                "variable.other.constant.property",
                "support.variable.property"
            ],
            "settings": {
                "foreground": "#C2C2C0",
                "fontStyle": ""
            }
        },

        {
            "scope": ["variable.language.this"],
            "settings": {
                "foreground": "#28bdca",
                "fontStyle": "italic"
            }
        },

        {
            "scope": ["variable.other.object.property"],
            "settings": {
                "foreground": "#9070E4",
                "fontStyle": ""
            }
        },

        {
            "scope": ["entity.name.type.module", "variable.other.object"],
            "settings": {
                "foreground": "#26c5d3",
                "fontStyle": ""
            }
        },

        {
            "scope": [
                "storage.type",
                "storage.type.function",
                "storage.type.class",
                "storage.type.function.arrow"
            ],
            "settings": {
                "foreground": "#EFB224",
                "fontStyle": "italic bold"
            }
        },
        /*
    
    
    CONSTANT.*          */
        {
            "scope": [
                "constant.character.escape",
                "constant.character",
                "constant",
                "constant.other",
                "constant.regexp",
                "constant.rgb-value",
                "constant.numeric",
                "constant.numeric.float",
                "constant.numeric.hex",
                "constant.numeric.integer",
                "constant.numeric.octal"
            ],
            "settings": {
                "foreground": "#CBDF48",
                "fontStyle": "bold"
            }
        },
        /*    

    CONSTANT.LANGUAGE.*             */
        {
            "scope": [
                "constant.language.undefined",
                "constant.language.null",
                "constant.language.boolean.true",
                "constant.language.boolean.false"
            ],
            "settings": {
                "foreground": "#F46A64",
                "fontStyle": "italic"
            }
        },
        /*
    
    KEYWORD.OPERATOR.*  [GROUP#1]           */
        {
            "scope": ["keyword.operator.assignment", "keyword.operator.comparison"],
            "settings": {
                "foreground": "#F47832",
                "fontStyle": ""
            }
        },

        {
            "scope": [
                "keyword.operator",
                "keyword.operator.arithmetic",
                "keyword.operator.increment",
                "keyword.operator.decrement",
                "keyword.operator.logical",
                "keyword.operator.ternary",
                "keyword.operator.rest",
                "keyword.operator.relational"
            ],
            "settings": {
                "foreground": "#F47832",
                "fontStyle": ""
            }
        },
        /*
    
    KEYWORD.OPERATOR.*  [GROUP#2]     */
        {
            "scope": [
                "keyword.operator.new",
                "keyword.operator.expression.typeof",
                "keyword.operator.expression.instanceof"
            ],
            "settings": {
                "foreground": "#F48040",
                "fontStyle": "italic"
            }
        },
        /*

    KEYWORD.CONTROL.*          */
        {
            "scope": [
                "keyword.control.conditional",
                "keyword.control.switch",
                "keyword.control.trycatch",
                "keyword.control.loop"
            ],
            "settings": {
                "foreground": "#F4964D",
                "fontStyle": ""
            }
        },

        /*
    
    STORAGE.MODIFIER.*      (+KEYWORD.CONTROL.)*/
        {
            "scope": [
                "keyword.control.flow",
                "storage.modifier",
                "storage.modifier.async"
            ],
            "settings": {
                "foreground": "#F0B02E",
                "fontStyle": ""
            }
        },
        /*

    
    PUNCUATION.*    (+META.BRACE.ROUND)         */
        {
            "scope": [
                "punctuation.definition.parameters",
                "meta.brace.square",
                "meta.brace.round",
                "punctuation",
                "punctuation.definition.block",
                "punctuation.definition.markdown",
                "punctuation.definition.tag",
                "punctuation.section",
                "punctuation.decorator"
            ],
            "settings": {
                "foreground": "#EED068",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "punctuation.definition.string",
                "punctuation.separator",
                "punctuation.terminator",
                "punctuation.accessor",
                "punctuation.definition.section.case-statement"
            ],
            "settings": {
                "foreground": "#DA8040",
                "fontStyle": "bold"
            }
        }
    ]
}
